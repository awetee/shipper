@model IEnumerable<Shipper2.Models.ProductCategory>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h3>Browse Product Categories</h3>
<p>
    Select from @Model.Count()
    genres:
</p>
<ul>
    @foreach (var productCategory in Model)
    {
        <li>@Html.ActionLink(productCategory.Name, "Browse", new { productCategory = productCategory.Name })</li>
    }
</ul>
